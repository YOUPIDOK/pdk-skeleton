{% if app.user.id != object.id and (
        (not object.isSuperAdmin and is_granted('ROLE_IMPERSONATE')) or
        (object.isSuperAdmin and is_granted('ROLE_SUPER_ADMIN'))
    )
%}
        <a
                href="{{ path('homepage', {_switch_user: object.email}) }}"
                class="btn btn-sm btn-default edit_link"
                title="Usurper {{ object.identity }}"
        >
                <i class="fas fa-eye" aria-hidden="true"></i>
                <span class="sr-only">Usurper</span>
        </a>
{% endif %}