{% if data is not empty %}
    <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
        <ol class="breadcrumb">
            {% for el in data %}
                <li class="breadcrumb-item {{ loop.last	 ? 'active aria-current="page"' : '"' }}">
                    {% if loop.last %}
                        {{  el is iterable ? el[1] : defaultPagesNames[el] }}
                    {% else %}
                        <a href="{{ path(el is iterable ? el[0] : el) }}">
                            {{  el is iterable ? el[1] : defaultPagesNames[el] }}
                        </a>
                    {% endif %}
                </li>
            {% endfor %}
        </ol>
    </nav>
{% endif %}